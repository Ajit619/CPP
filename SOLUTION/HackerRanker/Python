    a = int(input())
    b = int(input())
    c = a+b
    print(c)
    d = a-b
    print (d)
    e = a*b
    print (e)
    
# 4.Python : Division
_Ans_:
if __name__ == '__main__':
    a = int(input())
    b = int(input())
    c= a/b
    print (int(c))
    d= a/b
    print (float(d))

# 5. Loops
_Ans_:
if __name__ == '__main__':
    n = int(input())
    for num in range (0,n):
        square = num*num
        print(square)

# 6.Print Functiom
_Ans_:
if __name__ == '__main__':
    n = int(input())
    for value in range(1,n+1):
        print (value,end="")

# 7.Lists
_Ans_:
if __name__ == '__main__':
    N = int(input())
    Output = [];
    for i in range(0,N):
        ip = input().split();
        if ip[0] == "print":
            print(Output)
        elif ip[0] == "insert":
            Output.insert(int(ip[1]),int(ip[2]))
        elif ip[0] == "remove":
            Output.remove(int(ip[1]))
        elif ip[0] == "pop":
            Output.pop();
        elif ip[0] == "append":
            Output.append(int(ip[1]))
        elif ip[0] == "sort":
            Output.sort();
        else:
            Output.reverse();

